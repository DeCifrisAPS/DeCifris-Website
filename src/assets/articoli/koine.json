      { "id": "W14",
        "title": "Vote App: Verifiable and Coercion-Resistant I-Voting",
        "authors": [
          { "name": "Riccardo Longo" },
          { "name": "Chiara Spadafora" },
          { "name": "Francesco Antonio Marino" } ],
        "pageRange": "107-109",
